#!/system/bin/sh

BB=/res/bin/busybox;
INC=/data/PRIME-Kernel/bin/tweak.inc

if [ -e $INC ]; then
	echo 1 > /res/test.dat 2>/dev/null
	ERR=$?
	if [ $ERR -ne 0 ]; then
		$BB mount -t rootfs -o remount,rw rootfs
	else
		rm /res/test.dat 2>/dev/null
	fi
    source $INC
else
    P=`cat /res/synapse/settings/tweak_$1`;
    echo $P;
fi
